{
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}"],
  "coverageDirectory": "__coverage__",
  "rootDir": "..",
  "testPathIgnorePatterns": [
    "/__assets__/",
    "/__tests__/ts-types/",
    "/node_modules/"
  ],
  "transform": {
    "\\.(j|t)sx?$": "babel-jest"
  }
}
